{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"javascript": {
		"formatter": {
			"indentStyle": "space",
			"quoteStyle": "single",
			"jsxQuoteStyle": "single"
		}
	},
	"formatter": {
		"enabled": true
	},
	"linter": {
		"rules": {
			"suspicious": {
				"noExplicitAny": "warn",
				"noArrayIndexKey": "off"
			}
		}
	},
	"organizeImports": {
		"enabled": true
	}
}
